{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "TelegramServer": {
    "ApiToken": "Your Telegram Bot Api Token",
    "BotCommand": [
      {
        "command": "/start",
        "description": "Start the bot"
      },
      {
        "command": "/help",
        "description": "Get help"
      }
    ],
  },
  "Kestrel": {
    "Endpoints": {
      "MyHttpEndpoint": {
        "Url": "http://*:80"
      },
      "HttpsInlineCertFile": {
        "Url": "https://*:443",
        "Certificate": {
          "Path": "",
          "Password": ""
        }
      }
    }
  },
  "AllowedHosts": "*"
}
